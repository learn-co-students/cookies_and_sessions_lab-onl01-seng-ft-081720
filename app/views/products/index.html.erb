<body>
    <h1>Your cart:</h1> 
        <ul>
        <% cart.each do |item|  %>
            <li> <%= item %> </li>
            <% end %>
        </ul>
</body>

<h1>Add items to your cart:</h1>

<%= form_tag add_product_path do %>

<%= label_tag :product %><br>
<%= text_field_tag :'product' %> <br>

<%= submit_tag "add to cart" %>
<% end %>